{
  "execution": {
    "delete": true,
    "organize": false,
    "extract": false
  },
  "files": {
    "psd_handling": "convert",
    "pdf_handling": "convert",
    "clip_handling": "convert",
    "use_recycle_bin": false,
    "delete_archives": true
  },
  "delete_config": {
    "extensions": ["txt", "js", "url", "htm", "html", "docx"],
    "keywords": ["進捗", "宣伝", "同人誌", "予告", "新刊"]
  },
  "multiprocessing": {
    "enabled": true,
    "auto_adjust": true,
    "max_processes": {
      "psd": 8,
      "pdf": 16,
      "clip": 8
    }
  }
}